.x-mask {
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=30);
    opacity: 0.3;
    background: black;
}

 .r-button-close , .r-splitbutton-close .x-btn-split-right{
	background-image: url(images/icons/2.png);
}

.r-highlight-disabled .x-mask{
	filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=90);
    opacity: 0.9;
    background: transparent;
}

.x-panel-default-framed {
  -webkit-border-radius: 0px;
  -moz-border-radius: 0px;
  -ms-border-radius: 0px;
  -o-border-radius: 0px;
  border-radius: 0px;
  border-width: 1px;
}

.admin-viewport{
	background: url(images/square2.gif) repeat;
}

.admin-logo {
    background: url(images/logo03.png) no-repeat;
    width: 352px;
    height: 50px;
}

.admin-square{
	background: url(images/square.gif) repeat;
}

.login-viewport{
	background: url(images/square.gif) repeat;
}

.login-center{
	background-color:#053374;
}
.login-form{
	background: url(images/square.gif) repeat;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	-ms-border-radius:5px;
	-o-border-radius:5px;
	border-radius:5px;
}

.userlabel ,.userlabel .x-btn-inner{
	color:#FFFFFF
}

.login-form .x-panel-body{
	background:transparent;
}
.login-center-body{
	background: url(images/login.jpg) no-repeat;
	background-position:center;
}

.actioncolumn-margin{
	margin-left:5px;
}

.actioncolumn-img8{
	background:url(images/icons/8.png);
	margin-left:5px;
}


.actioncolumn-img16{
	background:url(images/icons/16.png);
	margin-left:5px;
}


.r-highlight-disabled .x-item-disabled .x-form-field ,
.r-highlight-disabled .x-item-disabled .x-form-item-label{
	filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=90);
    opacity: 0.9;
}


.ext-cal-evr, .ext-cal-evi, .ext-cal-evt dl {
    color: #1BA720;
}



.color-BFEFFF.x-grid-row-selected .x-grid-cell,
.color-7FFF00.x-grid-row-selected .x-grid-cell,
.color-FF00FF.x-grid-row-selected .x-grid-cell,
.color-FAEBD7.x-grid-row-selected .x-grid-cell,
.color-CCCCFF.x-grid-row-selected .x-grid-cell,
.color-84BF96.x-grid-row-selected .x-grid-cell {
    background-color: #c1ddf1 !important;
}

.color-BFEFFF .x-grid-cell{
	background-color:#BFEFFF;
}
.color-7FFF00 .x-grid-cell{
	background-color:#7FFF00;
}
.color-FF00FF .x-grid-cell{
	background-color:#FF00FF;
}
.color-84BF96 .x-grid-cell{
	background-color:#84BF96;
}
.color-FAEBD7 .x-grid-cell{
	background-color:#FAEBD7;
}
.color-CCCCFF .x-grid-cell{
	background-color:#CCCCFF;
}