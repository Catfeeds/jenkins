/*飞行计划、空域申请页面样式*/


.plan .title{
    padding:20px;
    color:#717171;
    border-bottom:solid 1px #9e9e9e;
    margin-bottom:10px;
}
.plan .title p{
    line-height:36px;
}

.plan .title button{
    width:100px;
}

.plan .tips{
    background:#f0f0f0;
    color:#7f7f7f;
    padding:5px 20px;
    margin-bottom:15px;
}

.plan-form{

}

.plan-form-title{
    position:relative;
    height:20px;
    line-height:20px;
}

.plan-form-title .text{
    position:absolute;
    top:0px;
    left:15px;
    font-size:16px;
    color:#bcbcbc;
    z-index: 2;
    padding:0 5px;
    background-color:#fff;
}

.plan-form-title .devider{
    position:absolute;
    top:50%;
    left:0;
    height:1px;
    width:100%;
    background-color:#f0f0f0;
    z-index: 1;
}

.plan-form-content{
    padding:20px;
}